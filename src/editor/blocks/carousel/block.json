{
    "apiVersion": 2,
    "name": "zthemename/carousel",
    "category": "media",
    "attributes": {
        "images": {
            "type": "array",
            "default": [],
            "source": "query",
            "selector": ".carousel-item",
            "query": {
                "url": {
                    "type": "string",
                    "source": "attribute",
                    "selector": "img",
                    "attribute": "src"
                },
                "alt": {
                    "type": "string",
                    "source": "attribute",
                    "selector": "img",
                    "attribute": "alt",
                    "default": ""
                },
                "id": {
                    "type": "string",
                    "source": "attribute",
                    "selector": "img",
                    "attribute": "data-id"
                },
                "caption": {
                    "type": "string",
                    "source": "html",
                    "selector": ".blocks-carousel-item__caption"
                }
            }
        },
        "ids": {
            "type": "array",
            "items": {
                "type": "number"
            },
            "default": []
        },
        "sizeSlug": {
            "type": "string",
            "default": "small"
        }
    },
    "supports": {
        "anchor": true
    },
    "editorStyle": "wp-block-zthemename-carousel-editor",
    "style": "wp-block-zthemename-carousel"
}
